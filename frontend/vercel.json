{
  "rewrites": [
    { "source": "/meeting/:id", "destination": "/api/share" },
    { "source": "/share/:id", "destination": "/api/share" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/meet/([^/]+)", "dest": "/index.html" },
    { "src": "/meeting/([^/]+)", "dest": "/index.html" },
    { "src": "/join/([^/]+)", "dest": "/index.html" },
    { "src": "/dashboard", "dest": "/index.html" },
    { "src": "/(.*)", "status": 200, "dest": "/index.html" }
  ],
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}
